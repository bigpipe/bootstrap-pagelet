{
  "name": "bootstrap-pagelet",
  "version": "1.0.8",
  "description": "Base Pagelet that is responsible for bootstrapping the client side of BigPipe",
  "main": "index.js",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- $(find test -name '*.test.js')"
  },
  "dependencies": {
    "pagelet": "0.9.x",
    "querystring": "0.2.x",
    "sapling": "1.0.x",
    "t": "0.5.x"
  },
  "devDependencies": {
    "assume": "1.2.x",
    "bigpipe": "0.9.x",
    "bigpipe.js": "0.9.x",
    "istanbul": "0.3.x",
    "mocha": "2.2.x",
    "pre-commit": "1.0.x",
    "temper": "0.3.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bigpipe/bootstrap-pagelet.git"
  },
  "keywords": [
    "Pagelet",
    "BigPipe",
    "Bootstrap",
    "base"
  ],
  "author": "Martijn Swaagman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigpipe/bootstrap-pagelet/issues"
  },
  "homepage": "https://github.com/bigpipe/bootstrap-pagelet"
}
